{
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "https://restaurant-analytics-backend.onrender.com/api/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "buildCommand": "npm run build",
  "outputDirectory": "build",
  "installCommand": "npm install"
}
